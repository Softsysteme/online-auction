pipeline {
	agent any
	
	stages {
		stage ('compile Stage') {
		
			steps {
				withMaven(maven : 'maven_3_5_0') {
					sh 'mvn clean install'
				}
			}
		}
			
		stage ('Testing Stage') {
		
			steps {
				withMaven(maven : 'maven_3_5_0') {
					sh 'mvn test'
				}
			}
		}
			
	stage ('Deplooyement Stage') {
		
			steps {
				withMaven(maven : 'maven_3_5_0') {
					sh 'mvn clean package docker:build'
				}
			}
		}		
			
					
				
